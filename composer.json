{
    "name": "kpdcdg/laravel-globelabs-api",
    "description": "A Laravel package for globelabs/globe-connect-php",
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/contracts": "5.5.*|5.6.*",
        "illuminate/support": "5.5.*|5.6.*",
        "php": "^7.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Kurt Dacudag",
            "email": "kurtperaldacudag@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Globe\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Globe\\GlobeServiceProvider"
            ],
            "aliases": {
                "Globe": "Globe\\Globe"
            }
        }
    }
}
